<mvc:View xmlns:tnt="sap.tnt" xmlns:f="sap.f" xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" controllerName="flexiblecolumnlayout.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page1" title="EMPLOYEE">
	<Bar>
		<contentRight>	
			<Button text="Download" icon="sap-icon://download" press="excelDownload" />
			<Button text="Upload" icon="sap-icon://upload" press="excelUpload"></Button>
		</contentRight>

	</Bar>
        <Table items="{/EmployeeExperience}" id="empExpTable">
			<columns>
				<Column ><Label text="ID"/></Column>
				<Column ><Label text="Comapny Name"/></Column>
				<Column ><Label text="Role"/></Column>
				<Column ><Label text="Start Date"/></Column>
				<Column ><Label text="End Date"/></Column>
				<Column ><Label text="Responsibilities"/></Column>
			</columns>
			<items>
				<ColumnListItem type="Navigation" press="NavToMid">
					<cells>
						<Text text="{ID}"></Text>
						<Text text="{CompanyName}"></Text>
						<Text text="{Role}"></Text>
						<Text text="{StartDate}" ></Text>
						<Text text="{EndDate}"></Text>
						<Text text="{Responsibilities}"></Text>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
    </Page>
</mvc:View>